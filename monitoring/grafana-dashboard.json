{
  "title": "API Performance Monitoring",
  "uid": "api-metrics-enhanced",
  "tags": [
    "api",
    "production",
    "mlops"
  ],
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "ðŸŽ¯ Total Predictions",
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "api_predictions_total",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "blue"
              },
              {
                "value": 20,
                "color": "green"
              },
              {
                "value": 50,
                "color": "yellow"
              }
            ]
          },
          "decimals": 0
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "âš¡ Model Inferences",
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 4,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "model_inference_seconds_count",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "purple"
              }
            ]
          },
          "decimals": 0
        }
      }
    },
    {
      "id": 3,
      "type": "gauge",
      "title": "â±ï¸ Avg Inference Time",
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 8,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "model_inference_seconds_sum / model_inference_seconds_count",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdMarkers": true,
        "showThresholdLabels": false
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "max": 3,
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 2,
                "color": "red"
              }
            ]
          },
          "decimals": 2
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "ðŸŒ Total HTTP Requests",
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(http_requests_total)",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "orange"
              }
            ]
          },
          "decimals": 0
        }
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "âœ… Success Rate",
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 16,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(http_requests_total{status=\"2xx\"}) / sum(http_requests_total) * 100",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 90,
                "color": "yellow"
              },
              {
                "value": 99,
                "color": "green"
              }
            ]
          },
          "decimals": 1
        }
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "ðŸ“Š Requests/Min",
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 20,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m])) * 60",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqpm",
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "cyan"
              }
            ]
          },
          "decimals": 1
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "ðŸ“ˆ Predictions Over Time",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "api_predictions_total",
          "legendFormat": "Total Predictions",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 30,
            "showPoints": "never"
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "âš¡ Request Rate (requests/min)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m])) * 60",
          "legendFormat": "Requests/min",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqpm",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "â±ï¸ Inference Time Trend",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "rate(model_inference_seconds_sum[1m]) / rate(model_inference_seconds_count[1m])",
          "legendFormat": "Avg Inference Time",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 25
          },
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 2,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "ðŸ” Requests by Endpoint",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum by(handler) (http_requests_total)",
          "legendFormat": "{{handler}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "barAlignment": 0
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 11,
      "type": "table",
      "title": "ðŸ“‹ Endpoint Statistics",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 22
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum by(handler, method, status) (http_requests_total)",
          "legendFormat": "{{handler}} {{method}} {{status}}",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "options": {
        "showHeader": true
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "id": 12,
      "type": "stat",
      "title": "ðŸš€ API Uptime",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 22
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(http_requests_total) > 0",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "name"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "value": "1",
              "text": "âœ… UP"
            }
          ]
        }
      }
    },
    {
      "id": 13,
      "type": "stat",
      "title": "ðŸ’¾ Total Data Processed",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 22
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "model_inference_seconds_count",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "green"
              }
            ]
          },
          "suffix": " inferences"
        }
      }
    }
  ]
}